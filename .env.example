# Supabase Configuration
# Note: In Next.js, client-side environment variables must be prefixed with NEXT_PUBLIC_
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Paystack Configuration
NEXT_PUBLIC_PAYSTACK_PUBLIC_KEY=pk_live_your_public_key_here
PAYSTACK_SECRET_KEY=sk_live_your_secret_key_here

# Resend Email API (optional - can use Supabase Edge Functions instead)
RESEND_API_KEY=your_resend_api_key
